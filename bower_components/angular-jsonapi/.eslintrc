{
  "extends": "eslint:recommended",
  "plugins": ["angular"],
  "env": {
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true
  },
  "rules": {
    "angular/definedundefined": 0,
    "angular/controller-as": 0,
    "angular/controller-as-route": 0,
    "angular/controller-as-vm": [0, "vm"]
  }
}
